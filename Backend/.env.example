### Copy to .env for local development
### NEVER commit real secrets. Render values go in Render -> Service -> Environment.

# Aiven MySQL (JDBC URL required)
DB_URL=jdbc:mysql://YOUR_AIVEN_HOST:YOUR_AIVEN_PORT/YOUR_DB_NAME?sslMode=REQUIRED
DB_USER=YOUR_DB_USER
DB_PASS=YOUR_DB_PASSWORD

# JWT
JWT_SECRET=CHANGE_ME
JWT_EXPIRATION=864000000

# Mail
MAIL_USERNAME=CHANGE_ME
MAIL_PASSWORD=CHANGE_ME

# Cloudinary
CLOUDINARY_CLOUD_NAME=CHANGE_ME
CLOUDINARY_API_KEY=CHANGE_ME
CLOUDINARY_API_SECRET=CHANGE_ME

# Frontend origin(s)
# Example: https://your-frontend.onrender.com
CORS_ALLOWED_ORIGINS=http://localhost:5173
